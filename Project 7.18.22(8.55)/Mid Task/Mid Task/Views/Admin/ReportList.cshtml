
@{
    ViewBag.Title = "ReportList";
}

<h2>Report List</h2>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Topic</th>
        <th>Details</th>
    </tr>
    @foreach (var d in Model)
    {
        <tr>
            <td>@d.Id</td>
            <td>@d.Name</td>
            <td>@d.Topic</td>
            <td>@d.Details</td>
            <td><a class="btn btn-info" href="/Admin/ReportDelete/@d.Id">Delete</a></td>
        </tr>
    }
</table>
<td><a class="btn btn-info" href="/Admin/AdminHome">Back</a></td>

