
@{
    ViewBag.Title = "HotelEdit";
}

<h2>HotelEdit</h2>


<form action="" method="Post">
    <table border="0" width="50%">
        <tr height=50>
            <td width="200px"> ID</td>
            <td width="300px">
                <input type="text" name="Id" value="@Model.Id">
            </td>
        </tr>
        <tr height=50>
            <td> Name </td>
            <td><input type="text" name="Name" value="@Model.Name"></td>
        </tr>
        <tr height=50>
            <td>Password</td>
            <td><input type="password" name="Password" value="@Model.Password"></td>
        </tr>
        <tr height=50>
            <td>Type</td>
            <td><input type="text" name="Type" value="@Model.Type"></td>
        </tr>
        <tr height=50>
            <td>Place</td>
            <td><input type="text" name="Place" value="@Model.Place"></td>
        </tr>
        <tr height=50>
            <td>Address</td>
            <td><input type="text" name="Address" value="@Model.Address"></td>
        </tr>
        <tr height=50>
            <td>Phone</td>
            <td><input type="text" name="Phone" value="@Model.Phone"></td>
        </tr>
        <tr height=50>
            <td>Email</td>
            <td><input type="email" name="Email" value="@Model.Email"></td>
            <td><input type="submit" name="submit" value="Submit"></td>
            <td><input type="reset" name="reset" value="Reset"></td>
        </tr>
    </table>
</form>
<td><a class="btn btn-info" href="/Admin/HotelList">Back</a></td>
