
@{
    ViewBag.Title = "Add";
}

<h2>Update</h2>
<form action="" method="post">
    <label for="id">ID: </label>
    <input type="text" name="ID" value="@Model.ID" class="form-control" />
    <label for="name">Name: </label>
    <input type="text" name="Name" value="@Model.Name" class="form-control" />
    <label for="Address">Address: </label>
    <input type="text" name="Address" value="@Model.Address" class="form-control" />
    <label for="Email">Email: </label>
    <input type="text" name="Email_ID" value="@Model.Email_ID" class="form-control" />
    <label for="Date_of_Birth">Date of Birth: </label>
    <input type="text" name="Date_of_birth" value="@Model.Date_of_birth" class="form-control" />
    <br />

    <input type="submit" value="Update" class="btn btn-primary" />
    <br />
    <a class="btn btn-info" href="/Accountant/Profile/">Back</a>


</form>
